  "knip": {
    "entry": [
      "index.js"
    ],
    "project": [
      "**/*.[jt]s?(x)"
    ],
    "ignore": [
      "**/*.d.ts",
      "**/*.config.js",
      "**/*.web.[jt]s?(x)",
      ".github/**/*"
    ],
    "rules": {
      "dependencies": "off",
      "binaries": "off",
      "unlisted": "off",
      "files": "warn",
      "unresolved": "warn",
      "exports": "warn",
      "nsExports": "warn",
      "types": "warn",
      "nsTypes": "warn",
      "enumMembers": "warn",
      "classMembers": "warn",
      "duplicates": "warn"
    }
  },
  "repository": "https://github.com/ethanneff/playground-react-native",
  "scripts": {
    "build": "react-scripts build",
    "ci-dupe-code": "jsinspect ./src",
    "ci-unused-code": "knip",
    "ci-env-vars": "mkdir -p certs; echo \"$ANDROID_STORE_FILE_RELEASE\" | base64 --decode > certs/release.jks; echo \"$ANDROID_STORE_FILE_DEBUG\" | base64 --decode > certs/debug.jks; echo \"$ANDROID_GOOGLE_KEY\" | base64 --decode > certs/google-key.json; echo \"$IOS_APPLE_KEY\" | base64 --decode > certs/apple-key.p8; ",
    "ci-install": "yarn install --frozen-lockfile",
    "ci-lint": "tsc && eslint './src/**/*.[jt]s?(x)' --cache --cache-location '.cache/eslint' --max-warnings=0 && prettier --check .",
    "ci-release": "semantic-release -e ./.github/release.config.js",
    "ci-test": "TZ=America/Los_Angeles jest --ci --bail --detectOpenHandles --maxWorkers=50% --silent --coverage --config .github/jest.config.js",
    "lint": "tsc; eslint './src/**/*.[jt]s?(x)' --cache --cache-location '.cache/eslint' --fix; prettier --write --cache --log-level silent .",
    "postinstall": "patch-package",
    "prepare": "husky install",
    "reset": "watchman watch-del-all; watchman shutdown-server; kill $(lsof -ti :8081); npx eslint . --fix; rm -rf /tmp/metro-bundler-cache-* ./android/build ./android/.gradle ./android/app/build ~/Library/Developer/Xcode/DerivedData /tmp/haste-map-react-native-packager-* ./ios/build ./ios/Pods/* ./ios/Podfile.lock ./ios/core.xcworkspace; xcrun simctl shutdown all; jest --clearCache; gem install fastlane; bundler update --all; bundler install; cd ios; pod update; cd ..; npx jetify; cd android; ./gradlew clean; cd ..; react-native start --reset-cache; npx react-native doctor; yarn rnx-align-check",
    "start": "react-native start --reset-cache",
    "test": "jest -u --cache --coverage --config .github/jest.config.js",
    "watch": "jest -u --watchAll --cache --config .github/jest.config.js",
    "web": "react-scripts start",
    "e2e-create": "maestro studio",
    "e2e-test": "maestro test .maestro/happy-path.yaml"
  },
  "version": "2.84.8"
}
