  "scripts": {
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "start": "react-native start --reset-cache",
    "web": "react-scripts start",
    "build": "react-scripts build",
    "lint": "tsc; eslint '**/*.[jt]s?(x)' --cache --fix; prettier --write --cache --loglevel silent .",
    "test": "jest -u --coverage --cache --config .github/jest.config.js",
    "watch": "jest -u --watchAll --cache --config .github/jest.config.js",
    "reset": "watchman watch-del-all; watchman shutdown-server; kill $(lsof -ti :8081); npx eslint . --fix; rm -rf /tmp/metro-bundler-cache-* ./android/build ./android/.gradle ./android/app/build ~/Library/Developer/Xcode/DerivedData /tmp/haste-map-react-native-packager-* ./ios/build ./ios/Pods/* ./ios/Podfile.lock ./ios/core.xcworkspace; xcrun simctl shutdown all; jest --clearCache; gem install fastlane; bundler update --all; bundler install; cd ios; pod update; cd ..; npx jetify; cd android; ./gradlew clean; cd ..; react-native start --reset-cache; npx react-native doctor; yarn rnx-dep-check",
    "ci-release": "semantic-release -e ./.github/release.config.js",
    "ci-install": "yarn install --frozen-lockfile",
    "ci-dupe": "jsinspect ./src",
    "ci-lint": "tsc && eslint '**/*.[jt]s?(x)' --cache --max-warnings=0 && prettier --check .",
    "ci-test": "TZ=America/Los_Angeles jest --ci --forceExit --detectOpenHandles --silent --coverage --config .github/jest.config.js",
    "postinstall": "patch-package",
    "prepare": "husky install"
  },
